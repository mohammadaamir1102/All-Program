package main;

public class BubbleShort {
	public static void main(String[] args) {
		int[] a = { 6, 0, 5, 2, 0, 1, 0, 3, 0, 4 };
		int temp;
		for (int i = 0; i < a.length; i++) {
			for (int j = i + 1; j < a.length; j++) {
				if (a[i] > a[j]) {
					temp = a[i];
					a[i] = a[j];
					a[j] = temp;

				}
			}

		}
		for (int i = 0; i < a.length; i++) {
			System.out.println(a[i]);
		}
	}

}
