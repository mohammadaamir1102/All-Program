package aamir;

class Aamir {
	public static void main(String[] args) {
		int number=123,rem,rev=0,pal;
		pal=number;
		while(number>0) {
			rem=number%10;
			rev=(rev*10)+rem;
			number=number/10;
		}
		if(rev==pal) {
			System.out.println("Palindrom number");
		}else {
			System.out.println("not palindrome number");
		}
	}
}
